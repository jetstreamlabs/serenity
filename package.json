{
  "private": true,
  "scripts": {
    "dev": "npm run build:assets && vite && npm run octane",
    "thin": "npm run build:assets && vite",
    "build": "npm run build",
    "prod": "npm run build:assets && vite build && vite build --ssr",
    "ssr": "vite build --ssr",
    "start": "node public/ssr/ssr.mjs",
    "kill": "kill -9 $(lsof -ti:3000)",
    "lint": "php ./vendor/bin/pinte --dirty",
    "pinte": "php ./vendor/bin/pinte",
    "build:assets": "php zen ziggy:generate && php zen zora:generate",
    "octane": "php zen octane:start --watch",
    "queue:start": "pm2 start horizon.json",
    "tests": "php ./vendor/bin/pest",
    "coverage": "XDEBUG_MODE=coverage php zen test --coverage --min=80",
    "cn": "composer update && npm update"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        120
      ],
      "type-enum": [
        2,
        "always",
        [
          "breaking",
          "feat",
          "fix",
          "task",
          "refactor",
          "docs",
          "chore"
        ]
      ]
    }
  },
  "dependencies": {
    "@inertiajs/inertia": "^0.11.1",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "@inertiajs/server": "^0.1.0",
    "@popperjs/core": "^2.11.6",
    "@vue/server-renderer": "^3.2.47",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "qs": "^6.11.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@headlessui/vue": "^1.7.12",
    "@iconify-json/heroicons-outline": "^1.1.6",
    "@iconify-json/heroicons-solid": "^1.1.7",
    "@inertiajs/progress": "^0.2.7",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/ziggy-js": "^1.3.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vueuse/core": "^9.13.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "laravel-echo": "^1.15.0",
    "laravel-vite-plugin": "^0.7.4",
    "lodash": "^4.17.21",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-shiki": "^0.8.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "pusher-js": "^8.0.1",
    "semantic-release": "^20.1.1",
    "tailwindcss": "^3.2.7",
    "unique-names-generator": "^4.7.1",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.1.4",
    "vite-plugin-vue-markdown": "^0.22.4",
    "vue": "^3.2.47",
    "vue-loader": "^17.0.1",
    "ziggy-js": "^1.5.0"
  }
}
