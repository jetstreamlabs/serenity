# APPLICATION
APP_NAME=Serenity
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_DOMAIN=localhost
APP_URL="http://${APP_DOMAIN}"

# LOGS
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# SNOWFLAKE
SNOWFLAKE_INSTANCE=snowflake
SNOWFLAKE_RESOLVER=laravel
SNOWFLAKE_DATACENTER=126
SNOWFLAKE_WORKER=22
SNOWFLAKE_MACHINE=6452

# SSR
LOCALE="en"

# VITE
VITE_APP_NAME="${APP_NAME}"
VITE_HTTPS=false
VITE_SSR_PORT=
VITE_DOMAIN="${APP_DOMAIN}"
VITE_CERTPATH=".config/valet/Certificates"

# OCTANE
OCTANE_HTTPS=true
OCTANE_SERVER=roadrunner

# DATABASE
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# DRIVERS
BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# PUSHER
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# CACHE
MEMCACHED_HOST=127.0.0.1

# REDIS
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# MAIL
MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@${APP_DOMAIN}"
MAIL_FROM_NAME="${APP_NAME}"

# AMAZON
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# HORIZON
HORIZON_PREFIX="serenity_horizon:"
HORIZON_PATH="jobs"
HORIZON_MEMORY_LIMIT=512
